  <text xml:lang="de">
    <body>
      <head rendition="#c #larger">X</head>
      <entry xml:lang="de" type="main" xml:id="E_x_1">
        <form type="lemma">
          <orth extent="full" rendition="#b">x</orth><pc rendition="#b">,</pc>
          <orth extent="full" rendition="#b">X</orth><pc rendition="#b">,</pc>
          <gramGrp>
            <gram type="pos" value="N"></gram>
            <gram type="determiner">das</gram><pc>;</pc>
            <gram type="genus" value="neuter"></gram>
            <gram type="genitive">-</gram><pc>,</pc>
            <gram type="plural">-</gram>
          </gramGrp>
        </form>
        <sense n="1." xml:id="S_x_2" rendition="#et">
          <def xml:id="N_x_1" rendition="#i">der vierundzwanzigste Buchstabe des Alphabets</def><pc>:</pc>
          <cit type="example">
            <quote>ein kleines, großes <ref type="headword">X</ref></quote><pc>;</pc>
          </cit>
          <cit type="example">
            <quote>ein anlautendes <ref type="headword">X</ref></quote><pc>;</pc>
          </cit>
          <cit type="example">
            <quote>das Wort wird mit <ref type="headword">x</ref> geschrieben</quote><pc>;</pc>
          </cit>
          <sense xml:id="S_x_2_10">
            <pc>/</pc><usg type="meaningType" expand="übertragen" rendition="#i">übertr.</usg><pc>;</pc>
            <note xml:id="N_x_19" rendition="#i">in der Wendung</note><pc>/</pc>
            <cit type="example">
              <quote>jmdm. ein <ref type="headword">X</ref> für ein U vormachen</quote>
              <pc>(</pc><gloss rendition="#i">jmdn. täuschen</gloss><pc>)</pc>
            </cit>
          </sense>
        </sense>
        <sense n="2." xml:id="S_x_3" rendition="#et">
          <pc>/</pc><def xml:id="N_x_2" rendition="#i">Bez. für einen unbekannten Namen, eine unbekannte Größe</def><pc>/</pc>
          <cit type="example">
            <quote>die Stadt <ref type="headword">X</ref></quote><pc>;</pc>
          </cit>
          <cit type="example">
            <quote>Herr <ref type="headword">X</ref></quote><pc>;</pc>
          </cit>
          <sense xml:id="S_x_3_10">
            <usg type="socioCultural" expand="umgangssprachlich" rendition="#g">umg.</usg>
            <def xml:id="N_x_3" rendition="#i">viele, unzählige</def><pc>:</pc>
            <cit type="example">
              <quote>wir haben uns seit <ref type="headword">x</ref> Jahren nicht mehr gesehen</quote><pc>;</pc>
            </cit>
            <cit type="example">
              <quote>sie hat <ref type="headword">x</ref> Bekannte, Verpflichtungen</quote><pc>;</pc>
            </cit>
          </sense>
          <sense xml:id="S_x_3_11">
            <usg type="time" expand="Neuprägung" rendition="#g">Neupräg.</usg>
            <cit type="example">
              <quote>der Tag <ref type="headword">X</ref></quote>
              <pc>(</pc><gloss rendition="#i">noch unbestimmter Tag, an dem etw. Entscheidendes geschehen soll</gloss><pc>)</pc><pc>;</pc>
            </cit>
          </sense>
          <sense xml:id="S_x_3_12">
            <usg type="domain" expand="Mathematik" rendition="#g">Math.</usg>
            <def xml:id="N_x_4" rendition="#i">Zeichen für eine unbekannte oder variable Größe</def><pc>:</pc>
            <cit type="example">
              <quote>die unbekannte Größe <ref type="headword">x</ref></quote><pc>;</pc>
            </cit>
            <cit type="example">
              <quote>einen (bestimmten) Wert für (die Größe) <ref type="headword">x</ref> einsetzen</quote>
            </cit>
          </sense>
        </sense>
      </entry>
      <entry xml:id="E_x_2_0" xml:lang="de" type="wordFamily">
        <form type="lemma" rendition="#b">
          <orth>x-</orth><pc>,</pc>
          <orth>X-</orth><pc>:</pc>
        </form>
        <entry xml:lang="de" type="relatedEntry" xml:id="E_x_2">
          <form type="lemma">
            <orth expand="x-Achse" extent="suff" rendition="#b">-=Achse</orth><pc rendition="#b">,</pc>
            <gramGrp>
              <gram type="pos" value="N"></gram>
              <gram type="determiner">die</gram>
              <gram type="genus" value="feminine"></gram>
            </gramGrp>
          </form>
          <usg type="domain" expand="Mathematik" rendition="#g">Math.</usg>
          <sense xml:id="S_x_4">
            <def xml:id="N_x_5" rendition="#i">Waagerechte im Koordinatensystem</def><pc>;</pc>
          </sense>
        </entry>
        <entry xml:lang="de" type="relatedEntry" xml:id="E_x_3">
          <form type="lemma">
            <orth expand="X-Beine" extent="suff" rendition="#b">-=Beine</orth><pc rendition="#b">,</pc>
            <gramGrp>
              <gram type="pos" value="N"></gram>
              <gram type="determiner">die</gram>
              <gram type="genus" value="feminine"></gram>
              <pc>/</pc><gram type="plural-only" rendition="#i">nur im Pl.</gram><pc>/</pc>
            </gramGrp>
          </form>
          <sense xml:id="S_x_5">
            <def xml:id="N_x_6" rendition="#i">nach innen gekehrte Knie</def><pc>:</pc>
            <cit type="example">
              <quote>er hat <ref type="headword" expand="X-Beine">X.</ref></quote><pc>;</pc>
            </cit>
          </sense>
        </entry>
        <entry xml:lang="de" type="relatedEntry" xml:id="E_x_4">
          <form type="lemma">
            <orth expand="X-beinig" extent="suff" rendition="#b">-=beinig</orth>
            <gramGrp>
              <pc>/</pc><gram type="pos" value="ADJ" rendition="#i">Adj.</gram><pc>;</pc>
            </gramGrp>
          </form>
          <sense xml:id="S_x_6">
            <xr type="related">
              <lbl rendition="#i">entsprechend der Bedeutung von</lbl>
              <ref type="sense" target="#S_x_5">-=Beine</ref><pc>/</pc><pc>:</pc>
            </xr>
            <cit type="example">
              <quote>ein <ref type="headword" expand="X-beiniges">X.</ref> Kind</quote><pc>;</pc>
            </cit>
            <cit type="example">
              <quote>sie läuft <ref type="headword" expand="X-beinig">X.</ref></quote><pc>;</pc>
            </cit>
          </sense>
        </entry>
        <entry xml:lang="de" type="relatedEntry" xml:id="E_x_5">
          <form type="lemma">
            <orth expand="x-beliebig" extent="suff" rendition="#b">-=beliebig</orth>
            <gramGrp>
              <pc>/</pc><gram type="pos" value="ADJ" rendition="#i">Adj.</gram><pc>;</pc>
              <note xml:id="N_x_20" rendition="#i">nur attr. u. adv.</note><pc>/</pc>
            </gramGrp>
          </form>
          <sense xml:id="S_x_7">
            <usg type="socioCultural" expand="umgangssprachlich" rendition="#g">umg.</usg>
            <def xml:id="N_x_7" rendition="#i">irgendein, irgendwer, ganz gleichgültig, was oder wer</def><pc>:</pc>
            <cit type="example">
              <!-- expand doesn't work here because the inflected forms differ for Buch and Mensch:
                   ein xbeliebiges Buch, ein x-beliebiger Mensch -->
              <quote>ein <ref type="headword">x.</ref> Buch, Mensch</quote><pc>;</pc>
            </cit>
            <cit type="example">
              <quote>eine <ref type="headword" expand="x-beliebige">x.</ref> Zahl</quote><pc>;</pc>
            </cit>
            <cit type="example">
              <quote>der Ernteertrag kann nicht <ref type="headword" expand="x-beliebig">x.</ref> gesteigert werden</quote><pc>;</pc>
            </cit>
          </sense>
        </entry>
        <entry xml:lang="de" type="relatedEntry" xml:id="E_x_6">
          <form type="lemma">
            <orth expand="x-mal" extent="suff" rendition="#b">-=mal</orth>
            <gramGrp>
              <pc>/</pc><gram type="pos" value="ADV" rendition="#i">Adv.</gram><pc>/</pc>
            </gramGrp>
          </form>
          <sense xml:id="S_x_8">
            <usg type="socioCultural" expand="umgangssprachlich" rendition="#g">umg.</usg>
            <def xml:id="N_x_8" rendition="#i">viele, unzählige Male</def><pc>:</pc>
            <cit type="example">
              <quote>etw. <ref type="headword" expand="x-mal">x.</ref> wiederholen</quote><pc>;</pc>
            </cit>
            <cit type="example">
              <quote>das habe ich schon <ref type="headword" expand="x-mal">x.</ref> gesagt</quote><pc>;</pc>
            </cit>
            <cit type="example">
              <quote>der Teppich ist schon <ref type="headword" expand="x-mal">x.</ref> ausgebessert</quote><pc>;</pc>
            </cit>
          </sense>
        </entry>
        <entry xml:lang="de" type="relatedEntry" xml:id="E_x_7">
          <form type="lemma">
            <orth expand="X-Strahlen" extent="suff" rendition="#b">-=Strahlen</orth><pc rendition="#b">,</pc>
            <gramGrp>
              <gram type="pos" value="N"></gram>
              <gram type="determiner">die</gram>
              <gram type="genus" value="feminine"></gram>
              <pc>/</pc><gram type="number" value="plural" rendition="#i">Pl.</gram><pc>;</pc>
              <gram type="plural-preferred" rendition="#i">Sg. ungebräuchl.</gram><pc>/</pc>
            </gramGrp>
          </form>
          <sense xml:id="S_x_9">
            <usg type="domain" expand="Physik" rendition="#g">Phys.</usg>
            <usg type="time" rendition="#g">veraltend</usg>
            <def xml:id="N_x_9" rendition="#i">Röntgenstrahlen</def>
          </sense>
        </entry>
      </entry>
      <entry xml:lang="de" type="main" xml:id="E_x_8">
        <form type="lemma">
          <orth norm="Xanthippe" extent="full" rendition="#b">Xanthịppe</orth><pc rendition="#b">,</pc>
          <gramGrp>
            <gram type="pos" value="N"></gram>
            <gram type="determiner">die</gram><pc>;</pc>
            <gram type="genus" value="feminine"></gram>
            <gram type="genitive">-</gram><pc>,</pc>
            <gram type="plural">-n</gram>
          </gramGrp>
        </form>
        <sense xml:id="S_x_10">
          <pc>/</pc><def xml:id="N_x_10" rendition="#i">die angeblich zanksüchtige Frau des griech. Philosophen Sokrates</def><pc>;</pc>
          <usg type="meaningType" expand="übertragen" rendition="#i">übertr.</usg><pc>/</pc>
          <usg type="socioCultural" expand="umgangssprachlich" rendition="#g">umg.</usg>
          <usg type="socioCultural" rendition="#g">abwertend</usg>
          <def xml:id="N_x_11" rendition="#i">bösartiges, streitsüchtiges Weib</def><pc>:</pc>
          <cit type="example">
            <quote>sie ist eine richtige <ref type="headword" expand="Xanthippe">X.</ref></quote>
          </cit>
        </sense>
      </entry>
      <entry xml:lang="de" type="main" xml:id="E_x_9">
        <form type="lemma">
          <orth norm="Xerographie" extent="full" rendition="#b">Xerographi̱e̱</orth><pc rendition="#b">,</pc>
          <gramGrp>
            <gram type="pos" value="N"></gram>
            <gram type="determiner">die</gram><pc>;</pc>
            <gram type="genus" value="feminine"></gram>
            <gram type="genitive">-</gram><pc>,</pc>
            <gram type="plural">-n</gram>
          </gramGrp>
          <pc>[</pc><pron>..i̱-en</pron><pc>]</pc>
        </form>
        <etym>
          <pc>&lt;</pc><lang rendition="#i">griech.</lang><pc>&gt;</pc>
        </etym>
        <sense xml:id="S_x_11">
          <usg type="time" expand="Neuprägung" rendition="#g">Neupräg.</usg>
          <usg type="domain" rendition="#g">Druckerei</usg>          
          <def xml:id="N_x_12" rendition="#i">lichtelektrisches Vervielfältigungs- und Druckverfahren, bei dem mit Hilfe der Photographie und elektrisch aufgeladener Metallplatten die Druckfarbe trocken aufgebracht wird</def>
        </sense>
      </entry>
      <entry xml:lang="de" type="main" xml:id="E_x_10">
        <form type="lemma">
          <orth extent="full" rendition="#b">x-fach</orth>
          <gramGrp>
            <pc>/</pc><gram type="pos" value="ADJ" rendition="#i">Adj.</gram><pc>/</pc>
          </gramGrp>
        </form>        
        <sense xml:id="S_x_12">
          <usg type="socioCultural" expand="umgangssprachlich" rendition="#i">umg.</usg>
          <def xml:id="N_x_13" rendition="#i">vielfach, unzählige Male</def><pc>:</pc>
          <cit type="example">
            <quote>etw. hat sich <ref type="headword" expand="x-fach">x.</ref> bewährt</quote><pc>;</pc>
          </cit>
          <cit type="example">
            <quote>ein <ref type="headword" expand="x-fach">x.</ref> erprobtes Waschmittel</quote><pc>;</pc>
          </cit>
          <cit type="example">
            <quote>das Ergebnis ist <ref type="headword" expand="x-fach">x.</ref> bestätigt worden</quote><pc>;</pc>
          </cit>
          <cit type="example">
            <quote>die Sammlung ergab das <ref type="headword">X-fache</ref> des Erwarteten</quote><pc>;</pc>
          </cit>
          <sense xml:id="S_x_12_10">
            <usg type="domain" expand="Mathematik" rendition="#g">Math.</usg>
            <cit type="example"><!-- rather a construction or prototype -->
              <quote>das X-fache</quote>
            </cit>
            <def xml:id="N_x_14" rendition="#i">die mit x vervielfachte Größe, Menge</def><pc>:</pc>
            <cit type="example">
              <quote>das <ref type="headword">X-fache</ref> einer Zahl errechnen</quote>
            </cit>
          </sense>
        </sense>
      </entry>
      <entry xml:lang="de" type="main" xml:id="E_x_11">
        <form type="lemma">
          <orth extent="full" rendition="#b">x-te</orth>
          <gramGrp>
            <pc>/</pc><gram type="pos" value="FIG" rendition="#i">Ord.zahl</gram><pc>;</pc>
          </gramGrp>
        </form>
        <sense xml:id="S_x_13">
          <note xml:id="N_x_21" rendition="#i">bezeichnet eine große, unbestimmte Anzahl</note><pc>/</pc>
          <cit type="example">
            <quote>ich habe es dir zum <ref type="headword" expand="x-ten">x.</ref> Male gesagt</quote><pc>;</pc>
          </cit>
          <sense xml:id="S_x_13_10">
            <usg type="domain" expand="Mathematik" rendition="#g">Math.</usg>
            <cit type="example">
              <quote>eine Zahl in die <ref type="headword" expand="x-te">x.</ref> Potenz erheben</quote>
            </cit>
          </sense>
        </sense>
      </entry>
      <entry xml:lang="de" type="main" xml:id="E_x_12">
        <form type="lemma">
          <orth norm="Xylographie" extent="full" rendition="#b">Xylographi̱e̱</orth><pc rendition="#b">,</pc>
          <gramGrp>
            <gram type="pos" value="N"></gram>
            <gram type="determiner">die</gram><pc>;</pc>
            <gram type="genus" value="feminine"></gram>
            <gram type="genitive">-</gram><pc>,</pc>
            <gram type="plural">-n</gram>
          </gramGrp>
          <pc>[</pc><pron>..ü..i̱-en</pron><pc>]</pc>
        </form>
        <etym>
          <pc>&lt;</pc><lang rendition="#i">griech.</lang><pc>&gt;</pc>
        </etym>        
        <sense n="1." xml:id="S_x_15" rendition="#et">
          <gramGrp>
            <pc>/</pc><gram type="singular-only" rendition="#i">ohne Pl.</gram><pc>/</pc>
          </gramGrp>
          <def xml:id="N_x_15" rendition="#i">Kunst des Holzschnitts</def>
        </sense>
        <sense n="2." xml:id="S_x_16" rendition="#et">
          <!-- actually, »1« in <ref> is typeset recte -->
          <def xml:id="N_x_16" rendition="#i">Produkt aus <xr type="related"><ref type="sense" target="#N_x_15">1</ref></xr>, Holzschnitt</def>
        </sense>
        <entry xml:lang="de" type="relatedEntry" xml:id="E_x_13">
          <xr type="related">
            <lbl rendition="#i">zu</lbl>
            <ref type="sense" target="#N_x_15">1</ref>
          </xr>          
          <form type="lemma">
            <orth norm="Xylograph" extent="full" rendition="#b">Xylogra̱ph</orth><pc rendition="#b">,</pc>
            <gramGrp>
              <gram type="pos" value="N"></gram>
              <gram type="determiner">der</gram><pc>;</pc>
              <gram type="genus" value="masculine"></gram>
              <gram type="genitive">-en</gram><pc>,</pc>
              <gram type="plural">-en</gram><pc>;</pc>
            </gramGrp>
          </form>
        </entry>
        <entry xml:lang="de" type="relatedEntry" xml:id="E_x_14">
          <xr type="related">
            <!-- note: this is an inferred link -->            
            <ref type="sense" target="#N_x_15"></ref>
          </xr>          
          <form type="lemma">
            <orth norm="xylographisch" extent="full" rendition="#b">xylogra̱phisch</orth>
            <gramGrp>
              <pc>/</pc><gram type="pos" value="ADJ" rendition="#i">Adj.</gram><pc>/</pc>
            </gramGrp>
          </form>
        </entry>
      </entry>      
      <entry xml:lang="de" type="main" xml:id="E_x_15">
        <form type="lemma">
          <orth norm="Xylophon" extent="full" rendition="#b">Xylopho̱n</orth><pc rendition="#b">,</pc>
          <gramGrp>
            <gram type="pos" value="N"></gram>
            <gram type="determiner">das</gram><pc>;</pc>
            <gram type="genus" value="neuter"></gram>
            <gram type="genitive">-s</gram><pc>,</pc>
            <gram type="plural">-e</gram>
          </gramGrp>
          <pc>[</pc><pron>..ü..</pron><pc>]</pc>
        </form>
        <etym>
          <pc>&lt;</pc><lang rendition="#i">griech.</lang><pc>&gt;</pc>
        </etym>
        <sense xml:id="S_x_17">
          <!-- actually, all numbers are typeset recte -->
          <def xml:id="N_x_17" rendition="#i">Schlaginstrument, das meist aus 36 in Tonleitern bis zu 5 Oktaven abgestimmten Holzstäben besteht, die in 4 nebeneinanderliegenden Reihen angeordnet sind und mit 2 hölzernen Schlegeln angeschlagen werden</def>
        </sense>
      </entry>
    </body>
  </text>
